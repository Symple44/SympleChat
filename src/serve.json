{
  "rewrites": [
    { "source": "/api/:path*", "destination": "http://192.168.0.15:8000/api/:path*" },
    { "source": "/**", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/**",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }
      ]
    }
  ]
}
